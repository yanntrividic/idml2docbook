      <phrase css:direction="ltr">Elle</phrase>
      <phrase css:direction="ltr">travaille</phrase>
      <phrase css:direction="ltr">pour</phrase>
      <phrase css:direction="ltr">sa</phrase>
      <phrase css:direction="ltr">famille</phrase> : <phrase css:direction="ltr">son</phrase>
      <phrase css:direction="ltr">père</phrase>, <phrase css:direction="ltr">sa</phrase>
      <phrase css:direction="ltr">mère</phrase>
      <phrase css:direction="ltr">et</phrase>
      <phrase css:direction="ltr">son</phrase>
      <phrase css:direction="ltr">fils</phrase>
      <phrase css:direction="ltr">au</phrase>
      <phrase css:direction="ltr">bled</phrase>. <phrase css:direction="ltr">Elle</phrase>
      <phrase css:direction="ltr">fait</phrase>
      <phrase css:direction="ltr">vivre</phrase>
      <phrase css:direction="ltr">toute</phrase>
      <phrase css:direction="ltr">la</phrase>
